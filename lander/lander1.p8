__lua__
function _init()
	make_player()
end

function _update()

end

function _draw()
	cls()
	draw_player()
end

function make_player()
	p={}
	p.x=60 --position
	p.y=8
	p.dx=0 --movement
	p.dy=0
	p.sprite=1
	p.alive=true
	p.thrust=0.075
end

function draw_player()
	spr(p.sprite,p.x,p.y)
end


	
	
